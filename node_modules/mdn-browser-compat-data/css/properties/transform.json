          "support": {
            "webview_android": {
              "prefix": "-webkit-",
              "version_added": "2.1",
              "notes": "Android 2.3 has a bug where input forms will \"jump\" when typing, if any container element has a <code>-webkit-transform</code>."
            },
            "chrome": [
              {
                "version_added": "36"
              },
              {
                "prefix": "-webkit-",
                "version_added": true
              }
            ],
            "chrome_android": {
              "prefix": "-webkit-",
              "version_added": true
            },
            "edge": {
              "prefix": "-webkit-",
              "version_added": "12"
            },
            "edge_mobile": {
              "prefix": "-webkit-",
              "version_added": true
            },
            "firefox": [
              {
                "version_added": "16"
              },
              {
                "prefix": "-webkit-",
                "version_added": "49"
              },
              {
                "prefix": "-webkit-",
                "version_added": "44",
                "flags": [
                  {
                    "type": "preference",
                    "name": "layout.css.prefixes.webkit",
                    "value_to_set": "true"
                  }
                ]
              }
            ],
            "firefox_android": [
              {
                "version_added": "16"
              },
