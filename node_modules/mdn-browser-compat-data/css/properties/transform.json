            "safari_ios": [
              {
                "version_added": "9"
              },
              {
                "prefix": "-webkit-",
                "version_added": "3.2"
              }
            ],
            "samsunginternet_android": {
              "version_added": true
            }
          },
          "status": {
            "experimental": false,
            "standard_track": true,
            "deprecated": false
          }
        },
        "3d": {
          "__compat": {
